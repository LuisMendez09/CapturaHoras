<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_cat_vehiculos"
        android:layout_width="298dp"
        android:layout_height="86dp"
        android:layout_marginTop="40dp"
        android:text="Actualizar vehiculos"
        app:layout_constraintEnd_toEndOf="@+id/btn_cat_tiposCombustibles"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_cat_tiposCombustibles"
        app:layout_constraintTop_toBottomOf="@+id/btn_cat_tiposCombustibles" />

    <Button
        android:id="@+id/btn_cat_tiposCombustibles"
        android:layout_width="297dp"
        android:layout_height="81dp"
        android:layout_marginTop="76dp"
        android:text="Actualizar restos de catalogos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>